{
  "name": "ipfs-gateway",
  "type": "module",
  "bin": "server.js",
  "scripts": {
    "build": "webpack --mode production --config webpack.config.mjs && pkg ./dist/bundle.js --target node18-linux-x64 -o ./dist/ipfs-gateway --public"
  },
  "pkg": {
    "assets": [
      "../firebase.json"
    ],
    "targets": [
      "node18-linux-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "express": "^4.21.0",
    "file-type": "^19.5.0",
    "firebase-admin": "^12.5.0",
    "kubo-rpc-client": "^5.0.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-import-attributes": "^7.25.6",
    "@babel/preset-env": "^7.25.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.2.1",
    "json-loader": "^0.5.7",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
